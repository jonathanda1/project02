<div class='container'>
<h1>Login</h1>


<%= form_tag sessions_path do %>
    <% flash.each do |key, msg| %>
        <div><%= msg %></div>
    <% end %>

    <form>
      <div>
          <%= label_tag :email %>
          <%= text_field_tag :email, params[:email] %>
      </div>
      <div>
          <%= label_tag :password %>
          <%= password_field_tag :password %>
      </div>
    </form>

    <%= submit_tag "Log in" %>
<% end %>
</div>
